#!/usr/bin/env python
import sys
import json

# read inputs.json
with open("/var/cache/opdemand/inputs.json") as f:
  data = f.read()
  inputs = json.loads(data)
  
# write out env vars for inclusion
with open("/var/cache/opdemand/inputs.sh", "w") as f:
  for k, v in iter(inputs.items()):
    key = k.replace("/", "_").lower()
    f.write("%s=\"%s\"\n" % (key, v))